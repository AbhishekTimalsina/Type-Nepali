{
  "manifest_version": 3,
  "name": "Type Nepali",
  "version": "1.0.0",
  "description": "Type in Nepali.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage", "scripting", "activeTab"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
    }
  ],

  "host_permissions": ["<all_urls>"]
}
